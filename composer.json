{
    "name": "adrianogf/clicksendsms",
    "description": "A Laravel package to send SMS using ClickSend API",
    "type": "library",
    "require": {
        "php": ">=8.0",
        "illuminate/support": "^9.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "clicksendsms\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ClickSendSms\\ClickSendSmsServiceProvider"
            ],
            "aliases": {
                "ClickSendSms": "ClickSendSms\\Facades\\ClickSendSms"
            }
        }
    }
}